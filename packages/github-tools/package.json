{
  "name": "@github-tools/sdk",
  "version": "0.0.1",
  "description": "GitHub tools for the Vercel AI SDK, wrap GitHub's API as AI SDK tools ready to plug into any agent",
  "author": "HugoRCD <hugo.richard@vercel.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoRCD/github-tools.git",
    "directory": "packages/github-tools"
  },
  "homepage": "https://github.com/HugoRCD/github-tools",
  "bugs": {
    "url": "https://github.com/HugoRCD/github-tools/issues"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "ai-sdk",
    "github",
    "tools",
    "agent"
  ],
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^22.0.1"
  },
  "peerDependencies": {
    "ai": "^6.0.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "tsdown": "^0.20.3",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm@10.29.3",
  "publishConfig": {
    "access": "public"
  }
}
